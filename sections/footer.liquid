<footer>
  <section class="container">
    <div class="row">
      <div class="col-xl-4 col-lg-4 col-md-8 col-sm-10 form-col">
        <h3>{{ settings.footer_title_one }}</h3>
        <form meathod="post" enctype="application/x-www-form-urlencoded" id="footer-form">
          <div class="form-wrapper">
            <div class="form-inner">
              <div class="form-row email-and-submit">

                <input type="email" placeholder="Your email" name="email" required>
                <div class="submit-wrapper">
                  <input type="submit" name="submit">

                  {% include 'icon-arrow-right' %}
                </div>

              </div>
            </div>
            <div class="form-success">
              <div class="inner">
                {% include 'icon-circle-check' %}
                <p>Thank you for subscribing!</p>
              </div>
            </div>
            <div class="form-failed">
              <div class="inner">
                {% include 'icon-sadface' %}
                <p>Something went wrong. Please refresh and try again.</p>
              </div>
            </div>

          </div>
        </form>

      <!--[if lte IE 8]>
        <script charset="utf-8" type="text/javascript" src="//js.hsforms.net/forms/v2-legacy.js"></script>
      <![endif]-->


      {% comment %} <script charset="utf-8" type="text/javascript" src="//js.hsforms.net/forms/v2.js"></script>
      <script>
      Â  hbspt.forms.create({
        portalId: "7407431",
        formId: "5350ad3d-a459-4c6c-bb8c-3202ff3affc1"
      });
      </script> {% endcomment %}
        
      </div>
      <div class="col-xl-2 col-lg-2 col-md-4 col-sm-6 offset-xl-1 offset-lg-1">
        <h3>{{ settings.footer_title_two }}</h3>
        <p>{{ linklists.settings.footer_menu_column_two.links }}</p>
        <p>{{ col_two_menu }}</p>
        <ul class="menu-plain">
          {% for link in linklists[settings.footer_menu_column_two].links %}
            <li>
              <a href="{{ link.url }}" 
              
              {% if settings.footer_menu_column_two == "footer-kinrgy" %} target="_blank" {% else %} target="_self {% endif %}
              "><span>{{ link.title }}</span>
                <i>{% include 'icon-menu-arrow-green' %}</i></a>
            </li>
          {% endfor %}
        </ul>
        
      </div>
      <div class="col-xl-2 col-lg-2 col-md-4 col-sm-6 offset-xl-1 offset-lg-1">
        <h3>{{ settings.footer_title_three }}</h3>
        <ul class="menu-plain">
          {% for link in linklists[settings.footer_menu_column_three].links %}
            <li>
              <a href="{{ link.url }}" target="_self">
                <span>{{ link.title }}</span>
                <i>{% include 'icon-menu-arrow-green' %}</i>
              </a>
            </li>
          {% endfor %}
        </ul>
        
      </div>
      <div class="col-xl-2 col-lg-2 col-md-4 col-sm-6 col-xl-offset-1">
        <h3>{{ settings.footer_title_four }}</h3>
        <ul class="menu-plain">
          {% for link in linklists[settings.footer_menu_column_four].links %}
            <li>
              <a href="{{ link.url }}" 
              {% if link.title contains "Facebook" or link.title contains "Instagram" %} target="_blank" {% else %} target="_self" {% endif %}
              ><span>{{ link.title }}</span>
                <i>{% include 'icon-menu-arrow-green' %}</i>
              </a>
            </li>
          {% endfor %}
        </ul>
        
      </div>

    </div>
    <ul class="row subfooter-list">
      {% if settings.footer_copyright != blank %}
        <li>
          <p class="small">{{ settings.footer_copyright }} {{ "today" | date: "%Y" }}.</p>
        </li>
      {% endif %}
      <li>
        {% comment %} <p class="small">Website by <a href="https://buzzworthystudio.com" target="_blank" class="inline-single">Buzzworthy</a></p> {% endcomment %}
        <a href="https://buzzworthystudio.com" target="_blank" class="bs" >Website by Buzzworthy <span class="label">Studio</span>
        <span class="bs-logo">
